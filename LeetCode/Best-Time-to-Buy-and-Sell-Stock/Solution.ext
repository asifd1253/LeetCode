class Solution {
    public int maxProfit(int[] prices) {
        int buyPrice = Integer.MAX_VALUE;
        int maxProfit = 0;

        for(int i=0; i<prices.length; i++){
            if(prices[i] > buyPrice){ //sell at high price day
                int sell = prices[i] - buyPrice;
                maxProfit = Math.max(maxProfit,sell);
            }else{
                buyPrice = prices[i];
                // buy at less price day
            }
        }

        return maxProfit;
    }
}